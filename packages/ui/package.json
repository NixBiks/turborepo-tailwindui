{
  "name": "ui",
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.0",
    "@types/react": "^18.0.3",
    "@types/react-dom": "^18.0.0",
    "concurrently": "^7.1.0",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "tailwindcss": "^3.0.24",
    "tsconfig": "workspace:*"
  },
  "scripts": {
    "build": "pnpm build:css && pnpm run build:js",
    "dev": "concurrently --names css,js --kill-others \"pnpm dev:css\" \"pnpm dev:js\"",
    "dev:js": "tsup src/Table/index.ts src/Layout/index.ts --watch --onSuccess \"tsc --emitDeclarationOnly --declaration\"",
    "dev:css": "pnpm tailwindcss -i ./src/input.css -o ./dist/main.css --watch",
    "build:css": "pnpm tailwindcss -i ./src/input.css -o ./dist/main.css",
    "build:js": "tsup src/Table/index.ts src/Layout/index.ts --dts"
  },
  "exports": {
    "./": "./dist/"
  },
  "typesVersions": {
    "*": {
      "main.css": [
        "main.css"
      ],
      "Layout": [
        "dist/Layout/index.d.ts"
      ],
      "Table": [
        "dist/Table/index.d.ts"
      ]
    }
  },
  "files": [
    "dist/main.css"
  ],
  "dependencies": {
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6"
  },
  "peerDependencies": {
    "react": "18.0.0",
    "react-dom": "18.0.0"
  }
}
